{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "baseUrl": "./src",
        "paths": {
            "@/configs/*": ["./configs/*"],
            "@/middlewares/*": ["./middlewares/*"],
            "@/models/*": ["./models/*"],
            "@/modules/*": ["./modules/*"],
            "@/routes/*": ["./routes/*"],
            "@/shared/*": ["./shared/*"],
            "@/src/*": ["./*"],
        }
    },
    "include": ["./src/**/*.js"],
    "exclude": ["node_modules", "**/node_modules/*"]
}
